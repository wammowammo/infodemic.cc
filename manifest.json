{
    "name": "Infodemic.cc",
    "version": "1.0.0",
    "description": "Scope out coordinated inauthentic behavior in the news",
    "manifest_version": 3,
    "author": "William Chen",
    "permissions": ["activeTab", "storage", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "/assets/icons/ailabs.png"
    }
}
